   "source": [
    "from functools import cmp_to_key\n",
    "\n",
    "# 系统排序函数 sorted 使用比较器修改比较规则\n",
    "def reverse_comparator(n1, n2):\n",
    "    return n2 - n1\n",
    "\n",
    "test = [3, 1, 2, 5, 9, 4, 7, 0]\n",
    "result  = sorted(test, key=cmp_to_key(reverse_comparator))\n",
    "print(test)\n",
    "print(result)\n"
   ]
